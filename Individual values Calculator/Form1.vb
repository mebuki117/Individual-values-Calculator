    '個性から最小個体値を取得
    Public Function Char_Optimize(IVs() As Integer, Status As Integer, L As Integer, Remainder As Integer) As Integer()
        IVs(Status + 1) = L
        For I = 1 To 11 Step 2
            If (L <= IVs(I)) Then IVs(I) = L
        Next

        For I = 0 To 31
            If (I Mod 5 = Remainder And I >= IVs(Status)) Then
                If (Status = 0 And I >= Math.Max(0, Math.Min(IVs(2), Math.Min(IVs(4), Math.Min(IVs(6), Math.Min(IVs(8), IVs(10))))))) Then IVs(Status) = I : Exit For
                If (Status = 2 And I >= Math.Max(0, Math.Min(IVs(0), Math.Min(IVs(4), Math.Min(IVs(6), Math.Min(IVs(8), IVs(10))))))) Then IVs(Status) = I : Exit For
                If (Status = 4 And I >= Math.Max(0, Math.Min(IVs(0), Math.Min(IVs(2), Math.Min(IVs(6), Math.Min(IVs(8), IVs(10))))))) Then IVs(Status) = I : Exit For
                If (Status = 6 And I >= Math.Max(0, Math.Min(IVs(0), Math.Min(IVs(2), Math.Min(IVs(4), Math.Min(IVs(8), IVs(10))))))) Then IVs(Status) = I : Exit For
                If (Status = 8 And I >= Math.Max(0, Math.Min(IVs(0), Math.Min(IVs(2), Math.Min(IVs(4), Math.Min(IVs(6), IVs(10))))))) Then IVs(Status) = I : Exit For
                If (Status = 10 And I >= Math.Max(0, Math.Min(IVs(0), Math.Min(IVs(2), Math.Min(IVs(4), Math.Min(IVs(6), IVs(8))))))) Then IVs(Status) = I : Exit For
            End If
        Next
        Return IVs

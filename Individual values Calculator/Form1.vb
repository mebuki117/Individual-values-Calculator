    '個性から個体値を取得
    Public Function Char_Conversion(Char_ As String, IVs() As Integer) As Integer()
        Dim L As Integer

        If (Char_ = "#未指定") Then Return IVs

        For L = 31 To 0 Step -1
            If (Char_ = "たべるのがだいすき" And L Mod 5 = 0 And L <= IVs(1)) Then IVs = Char_Optimize(IVs, 0, L, 0) : Exit For

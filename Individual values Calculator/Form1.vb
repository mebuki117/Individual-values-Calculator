    '====================================================================================================================================================================

    '計算

    '====================================================================================================================================================================

    Private Sub Calculation_Click(sender As Object, e As EventArgs) Handles Calculation.Click

        '可能性のあるめざパのリセット
        Calced_Power_and_Type.Items.Clear()
        Calced_Power_and_Type.Update()

        'ポケモンデータの取得
        Dim Name_Index As Integer = Array.IndexOf(Pokemon_Basics, Pokemon.Text)
        Calced_Pokemon.Text = Pokemon_Basics(Name_Index)
        Calced_Pokemon.Update()
        Dim BSs(5) As Integer
        For L = 0 To 5
            BSs(L) = Pokemon_Basics(Name_Index + L + 1)
        Next


        'CSVデータからポケモン名のみ取得（新規データ追加用）
        'Dim CSVGet As String = ""
        'For L = 0 To 1000
        'CSVGet += Pokemon_Basics((L * 9) + 1) + ","
        'Clipboard.Clear()
        'If (Pokemon_Basics((L * 9) + 1) = "ゲノセクト") Then Exit For
        'Next
        'Clipboard.SetText(CSVGet)


        '可能性のある個体値の最小値と最大値を求める
        If (Level.Text = "") Then Level.Text = 1

        If (HP_RSs.Text = "") Then HP_RSs.Text = 0
        If (HP_EVs.Text = "") Then HP_EVs.Text = 0

        If (A_RSs.Text = "") Then A_RSs.Text = 0
        If (A_EVs.Text = "") Then A_EVs.Text = 0

        If (B_RSs.Text = "") Then B_RSs.Text = 0
        If (B_EVs.Text = "") Then B_EVs.Text = 0

        If (C_RSs.Text = "") Then C_RSs.Text = 0
        If (C_EVs.Text = "") Then C_EVs.Text = 0

        If (D_RSs.Text = "") Then D_RSs.Text = 0
        If (D_EVs.Text = "") Then D_EVs.Text = 0

        If (S_RSs.Text = "") Then S_RSs.Text = 0
        If (S_EVs.Text = "") Then S_EVs.Text = 0


        '変数代入
        Dim Type_Total As Integer = 0
        Dim Power_Total As Integer = 0
        Dim Lv As Integer = Level.Text
        Dim RSs() As Integer = {HP_RSs.Text, A_RSs.Text, B_RSs.Text, C_RSs.Text, D_RSs.Text, S_RSs.Text}
        Dim EVs() As Integer = {HP_EVs.Text, A_EVs.Text, B_EVs.Text, C_EVs.Text, D_EVs.Text, S_EVs.Text}
        Dim IVs_MimMax() As Integer = {99, 99, 99, 99, 99, 99, 99, 99, 99, 99, 99, 99} 'HP最小, HP最大, A最小, A最大...
        Dim Nature_revise() As Double = {1, 1, 1, 1, 1}


        '努力値警告
        If (510 <= EVs(0) + EVs(1) + EVs(2) + EVs(3) + EVs(4) + EVs(5)) Then
            If MessageBox.Show("努力値の合計が510を超えていますが、この状態で計算しますか？", "努力値が不正です", MessageBoxButtons.YesNo, MessageBoxIcon.Exclamation) = DialogResult.No Then Exit Sub
        End If


        '性格補正の適応
        For L = 0 To 4
            Nature_revise(L) = Nature_Conversion(Nature.Text)(L)
        Next


        '可能性のある個体値の最小値と最大値を求める
        Dim Remainder As Integer = 0
        If (HP_IVs_Enabled.Checked = False) Then
            For L = 0 To 31
                If (HP_Real_Value(BSs(0), L, EVs(0), Lv) = RSs(0)) Then
                    IVs_MimMax(0) = Math.Min(IVs_MimMax(0), L)
                    IVs_MimMax(1) = Math.Max(IVs_MimMax(0), L)
                End If
            Next
        Else
            IVs_MimMax(0) = HP_IVs.Text
            IVs_MimMax(1) = HP_IVs.Text
        End If

        If (A_IVs_Enabled.Checked = False) Then
            For L = 0 To 31
                If (A_S_Real_Value(BSs(1), L, EVs(1), Lv, Nature_revise(0)) = RSs(1)) Then
                    IVs_MimMax(2) = Math.Min(IVs_MimMax(2), L)
                    IVs_MimMax(3) = Math.Max(IVs_MimMax(2), L)
                End If
            Next
        Else
            IVs_MimMax(2) = A_IVs.Text
            IVs_MimMax(3) = A_IVs.Text
        End If

        If (B_IVs_Enabled.Checked = False) Then
            For L = 0 To 31
                If (A_S_Real_Value(BSs(2), L, EVs(2), Lv, Nature_revise(1)) = RSs(2)) Then
                    IVs_MimMax(4) = Math.Min(IVs_MimMax(4), L)
                    IVs_MimMax(5) = Math.Max(IVs_MimMax(4), L)
                End If
            Next
        Else
            IVs_MimMax(4) = B_IVs.Text
            IVs_MimMax(5) = B_IVs.Text
        End If

        If (C_IVs_Enabled.Checked = False) Then
            For L = 0 To 31
                If (A_S_Real_Value(BSs(3), L, EVs(3), Lv, Nature_revise(2)) = RSs(3)) Then
                    IVs_MimMax(6) = Math.Min(IVs_MimMax(6), L)
                    IVs_MimMax(7) = Math.Max(IVs_MimMax(6), L)
                End If
            Next
        Else
            IVs_MimMax(6) = C_IVs.Text
            IVs_MimMax(7) = C_IVs.Text
        End If

        If (D_IVs_Enabled.Checked = False) Then
            For L = 0 To 31
                If (A_S_Real_Value(BSs(4), L, EVs(4), Lv, Nature_revise(3)) = RSs(4)) Then
                    IVs_MimMax(8) = Math.Min(IVs_MimMax(8), L)
                    IVs_MimMax(9) = Math.Max(IVs_MimMax(8), L)
                End If
            Next
        Else
            IVs_MimMax(8) = D_IVs.Text
            IVs_MimMax(9) = D_IVs.Text
        End If

        If (S_IVs_Enabled.Checked = False) Then
            For L = 0 To 31
                If (A_S_Real_Value(BSs(5), L, EVs(5), Lv, Nature_revise(4)) = RSs(5)) Then
                    IVs_MimMax(10) = Math.Min(IVs_MimMax(10), L)
                    IVs_MimMax(11) = Math.Max(IVs_MimMax(10), L)
                End If
            Next
        Else
            IVs_MimMax(10) = S_IVs.Text
            IVs_MimMax(11) = S_IVs.Text
        End If


        'ヌケニンの場合、可能性のあるHP個体値を0~31にする
        If (Pokemon_Basics(Name_Index) = "ヌケニン") Then
            IVs_MimMax(0) = 0
            IVs_MimMax(1) = 31
        End If


